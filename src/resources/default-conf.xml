<?xml version="1.0" encoding="ISO-8859-15" standalone="no"?>
<conf>
    <version>0.9</version>
    <modules>
        <search>true</search>
        <dashboard>true</dashboard>
        <deckbuilder>true</deckbuilder>
        <collection>true</collection>
        <game>true</game>
        <rss>false</rss>
        <alarm>true</alarm>
        <shopper>true</shopper>
        <cardbuilder>true</cardbuilder>
        <stock>true</stock>
    </modules>
    <langage>English</langage>
   	<lookAndFeel>javax.swing.plaf.nimbus.NimbusLookAndFeel</lookAndFeel>
    <debug>INFO</debug>
    <tooltip>true</tooltip>
    <default-library>Library</default-library>
    <loglevel>DEBUG</loglevel>
    <default-website-dir/>
    <keystore-name>jssecacerts</keystore-name>
    <keystore-pass>changeit</keystore-pass>
    <default-land-deck>ZEN</default-land-deck>
    <caches>
        <cache>
            <class>org.magic.api.cache.impl.FileCache</class>
            <enable>true</enable>
        </cache>
        <cache>
            <class>org.magic.api.cache.impl.MemoryCache</class>
            <enable>false</enable>
        </cache>
        <cache>
            <class>org.magic.api.cache.impl.NoCache</class>
            <enable>false</enable>
        </cache>
    </caches>
    <servers>
        <server>
            <class>org.magic.servers.impl.ConsoleServer</class>
            <enable>true</enable>
        </server>
        <server>
            <class>org.magic.servers.impl.JSONHttpServer</class>
            <enable>true</enable>
        </server>
        <server>
            <class>org.magic.servers.impl.PricesCheckerTimer</class>
            <enable>true</enable>
        </server>
        <server>
            <class>org.magic.servers.impl.MTGGameRoomServer</class>
            <enable>true</enable>
        </server>
    </servers>
    <pricers>
        <pricer>
            <class>org.magic.api.pricers.impl.MagicCardMarketPricer2</class>
            <enable>false</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.EbayPricer</class>
            <enable>false</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.MagicTradersPricer</class>
            <enable>false</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.MagicVillePricer</class>
            <enable>true</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.TCGPlayerPricer</class>
            <enable>true</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.ChannelFireballPricer</class>
            <enable>false</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.DeckTutorPricer</class>
            <enable>false</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.MTGPricePricer</class>
            <enable>true</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.CardKingdomPricer</class>
            <enable>true</enable>
        </pricer>
        <pricer>
            <class>org.magic.api.pricers.impl.MagicBazarPricer</class>
            <enable>true</enable>
        </pricer>
    </pricers>
    <providers>
        <provider>
            <class>org.magic.api.providers.impl.MtgjsonProvider</class>
            <enable>true</enable>
        </provider>
        <provider>
            <class>org.magic.api.providers.impl.DeckbrewProvider</class>
            <enable>false</enable>
        </provider>
        <provider>
            <class>org.magic.api.providers.impl.MagicTheGatheringIOProvider</class>
            <enable>false</enable>
        </provider>
        <provider>
            <class>org.magic.api.providers.impl.MtgapiProvider</class>
            <enable>false</enable>
        </provider>
        <provider>
            <class>org.magic.api.providers.impl.PrivateMTGSetProvider</class>
            <enable>false</enable>
        </provider>
        <provider>
            <class>org.magic.api.providers.impl.ScryFallProvider</class>
            <enable>false</enable>
        </provider>
    </providers>
    <daos>
        <dao>
            <class>org.magic.api.dao.impl.HsqlDAO</class>
            <enable>true</enable>
        </dao>
        <dao>
            <class>org.magic.api.dao.impl.MysqlDAO</class>
            <enable>false</enable>
        </dao>
        <dao>
            <class>org.magic.api.dao.impl.PostgresqlDAO</class>
            <enable>false</enable>
        </dao>
        <dao>
            <class>org.magic.api.dao.impl.FileDAO</class>
            <enable>false</enable>
        </dao>
    </daos>
    <pictures>
        <picture>
            <class>org.magic.api.pictures.impl.GathererPicturesProvider</class>
            <enable>true</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.MagicCardInfoPicturesProvider</class>
            <enable>false</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.PersonalSetPicturesProvider</class>
            <enable>false</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.DeckMasterPicturesProvider</class>
            <enable>false</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.ScryFallPicturesProvider</class>
            <enable>false</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.MagidexPicturesProvider</class>
            <enable>false</enable>
        </picture>
        <picture>
            <class>org.magic.api.pictures.impl.MythicSpoilerPicturesProvider</class>
            <enable>false</enable>
        </picture>
    </pictures>
    <shoppers>
        <shopper>
            <class>org.magic.api.shopping.impl.LeboncoinShopper</class>
            <enable>true</enable>
        </shopper>
        <shopper>
            <class>org.magic.api.shopping.impl.EbayShopper</class>
            <enable>false</enable>
        </shopper>
        <shopper>
            <class>org.magic.api.shopping.impl.PriceMinisterShopper</class>
            <enable>false</enable>
        </shopper>
    </shoppers>
    <dashboards>
        <dashboard>
            <class>org.magic.api.dashboard.impl.MTGoldFishDashBoard</class>
            <enable>true</enable>
        </dashboard>
        <dashboard>
            <class>org.magic.api.dashboard.impl.MTGPriceDashBoard</class>
            <enable>false</enable>
        </dashboard>
        <dashboard>
            <class>org.magic.api.dashboard.impl.MTGStockDashBoard</class>
            <enable>false</enable>
        </dashboard>
    </dashboards>
    <decksniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.TappedOutDeckSniffer</class>
            <enable>true</enable>
        </sniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.MTGoldFishDeck</class>
            <enable>false</enable>
        </sniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.MagicCorporationDecks</class>
            <enable>false</enable>
        </sniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.LotusNoirDecks</class>
            <enable>true</enable>
        </sniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.DeckstatsDeckSniffer</class>
            <enable>true</enable>
        </sniffer>
        <sniffer>
            <class>org.magic.api.decksniffer.impl.TCGPlayerDeckSniffer</class>
            <enable>true</enable>
        </sniffer>
    </decksniffer>
    <deckexports>
        <export>
            <class>org.magic.api.exports.impl.CocatriceDeckExport</class>
            <enable>true</enable>
        </export>
        <export>
