        <export>
            <class>org.magic.api.exports.impl.PDFExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.MTGDesktopCompanionExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.MTGODeckExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.JsonExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.OCTGNDeckExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.DCIDeckSheetExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.MKMFileWantListExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.Apprentice2DeckExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.MkmOnlineExport</class>
            <enable>true</enable>
        </export>
        <export>
            <class>org.magic.api.exports.impl.XMageDeckExport</class>
            <enable>true</enable>
        </export>
    </deckexports>
    <rss>
        <flux>
            <category>Spoil</category>
            <name>Magic Spoiler</name>
            <url>http://www.magicspoiler.com/feed/</url>
        </flux>
        <flux>
            <category>Spoil</category>
            <name>MTGSalvation</name>
            <url>http://www.mtgsalvation.com/spoilers.rss</url>
        </flux>
        <flux>
            <category>News En</category>
            <name>Channel Fireball</name>
            <url>http://www.channelfireball.com/feed/</url>
        </flux>
        <flux>
            <category>News En</category>
            <name>Daily MTG</name>
            <url>http://magic.wizards.com/rss/rss.xml</url>
        </flux>
        <flux>
            <category>News En</category>
            <name>MTGSalvation</name>
            <url>http://www.mtgsalvation.com/articles.rss</url>
        </flux>
        <flux>
            <category>News FR</category>
            <name>MagicTrade</name>
            <url>https://magictrade.org/articles/infos-rumeurs/feed/</url>
        </flux>
        <flux>
            <category>News FR</category>
            <name>MagicCorporation</name>
            <url>http://www.magiccorporation.com/gathering-news-last-rss.html</url>
        </flux>
        <flux>
            <category>News FR</category>
            <name>SMFCorp</name>
            <url>http://www.smfcorp.net/rss/dataRSS.php?listId=1</url>
        </flux>
        <flux>
            <category>Tradding</category>
            <name>MTGGoldFish</name>
            <url>http://www.mtggoldfish.com/feed.rss</url>
        </flux>
    </rss>
    <min-price-alert/>
    <debug-json-panel>false</debug-json-panel>
    <game>
        <cards>
            <card-width>154</card-width>
            <card-height>215</card-height>
        </cards>
        <player-profil>
            <name>nicho</name>
            <foreground/>
            <background/>
        </player-profil>
    </game>
</conf>
